{
  "root": true,
  "env": {
    "node": true
  },
  "extends": [
    "plugin:vue/vue3-recommended",
    "eslint:recommended"
  ],
  "parserOptions": {
    "parser": "@babel/eslint-parser"
  },
  "rules": {
    "vue/html-self-closing": 0,
    "vue/singleline-html-element-content-newline": 0,
    "vue/max-attributes-per-line": 0
  }
}
